
.details{
  padding: 20rpx 20rpx 6rpx 20rpx;
  font-family: 楷体;
  font-size: 27rpx;
}
.dtext{
  font-size: 32rpx;
  font-weight: bold;
  display: block;
  margin: 0 auto;
  text-align: center;
  margin-bottom: 5rpx;
}
.detailsImg{
  display: flex;
  justify-content: space-between;
  margin-top: 12rpx;
}
.detailsImg image{
  width: 354rpx;
  height: 195rpx;
}


.lunbo {
  width: 100%;
  height: 450rpx;
  display: block;
}
.u-img-slide {
  width: 100%;
  height: inherit;
}



page {
  position: relative;
  box-sizing: border-box;
}

.view-page {
  height: 135rpx;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  /* position: absolute; */
  overflow: hidden;
  background: linear-gradient(to right,rgb(255,123,25),rgb(255,249,1));
}

.page-content {
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
  color: #fff;
  font-family: 宋体;
  font-size: 36rpx;
  font-weight: bold;
  top: 70rpx;
  left: 30rpx;
}

.title{
  margin-left: 200rpx
}
.fanhui{
  position: absolute;
  top: 9rpx;left: 17rpx;
  width: 20rpx;
  height: 20rpx;
  border-bottom: 7rpx solid #fff;
  border-left: 7rpx solid #fff;
  transform: rotate(45deg);
}