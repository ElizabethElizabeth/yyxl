page {
  position: relative;
  box-sizing: border-box;
  background-color: #fff;
}
.view-page {
  height: 135rpx;
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  box-sizing: border-box;
  overflow: hidden;
  background: linear-gradient(to right,rgb(255,123,25),rgb(255,249,1));
}
.page-content {
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
  height: 50rpx;
  color: #fff;
  font-family: 宋体;
  font-size: 36rpx;
  font-weight: bold;
  top: 65rpx;
  left: 30rpx;
  margin-bottom: 20rpx;
}
.title{
  margin-left: 200rpx;
  line-height: 50rpx;
}
.fanhui{
  position: absolute;
  top: 0rpx;left: 0rpx;
  width: 50rpx;
  height: 50rpx;
}




.tab{
  display: flex;
  font-size: 32rpx;
  color:#000000;
  background: #f4f4f4;
  margin-top: 135rpx;
}
.tabc{
  width: 49.9%;
}
.tab-item{
  width: 36%;
  text-align: center;
  margin: 6% 32%;
}
.tab-item.active{
  color: #ff9115;
  font-weight: bold;
  border-bottom: 7rpx solid #ff9115;
}



